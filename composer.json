{
    "name": "ankushtyagi/laravel-media-library",
    "description": "A powerful media library manager for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ankush Tyagi",
            "email": "getankushtyagi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "*",
        "intervention/image": "^2.7"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ankushtyagi\\MediaLibrary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ankushtyagi\\MediaLibrary\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ankushtyagi\\MediaLibrary\\MediaLibraryServiceProvider"
            ],
            "aliases": {
                "MediaLibrary": "Ankushtyagi\\MediaLibrary\\Facades\\MediaLibrary"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
